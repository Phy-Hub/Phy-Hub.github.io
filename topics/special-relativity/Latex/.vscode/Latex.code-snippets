		"\t\\begin{subfigure}{0.9\\textwidth}",
		"\t\t\\centering",
		"\t\t\\includegraphics[width=\\textwidth]{images/pdf/${2:first figure name}}",
		"\t\t\\caption{${3: first sub caption}}",
		"\t\t\\label{fig: ${1:title} subfig_1}",
		"\t\\end{subfigure}",
		"\t\\begin{subfigure}{0.9\\textwidth}",
		"\t\t\\centering",
		"\t\t\\includegraphics[width=\\textwidth]{images/pdf/${4: second figure name}}",
		"\t\t\\caption{${5:sub caption}}",
		"\t\t\\label{fig: ${1:title} subfig_2}",
		"\t\\end{subfigure}",
		"\t\\caption{Diagram ${6:main caption}.}",
