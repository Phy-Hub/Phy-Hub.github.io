{
	// Place your Latex workspace snippets here. Each snippet is defined under a snippet name and has a scope, prefix, body and
	// description. Add comma separated ids of the languages where the snippet is applicable in the scope field. If scope
	// is left empty or omitted, the snippet gets applied to all languages. The prefix is what is
	// used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders.
	// Placeholders with the same ids are connected.
	// Example:

	"figure":
	{
		"prefix": "l.figure",
		"body":
		[
		"\\begin{figure}[H]",
		"\t\\figuretitle{${1:title}}",
		"\t\\centering",
		"\t\\includegraphics[width=0.9\\textwidth]{images/pdf/${2:filename}}",
		"\t\\caption{Diagram ${3:caption}.}",
		"\t\\label{fig: ${1:title}}",
		"\\end{figure}"
		],
	},

	"figures_2":
	{
		"prefix": "l.figures_two",
		"body":
		[
		"\\begin{figure}[H]",
		"\t\\figuretitle{${1:title}}",
		"\t\\centering",
		"\t\\begin{subfigure}{0.45\\textwidth}",
		"\t\t\\centering",
		"\t\t\\includegraphics[width=\\textwidth]{images/pdf/${2:first figure name}}",
		"\t\t\\caption{${3: first sub caption}}",
		"\t\t\\label{subfig_1: ${1:title}}",
		"\t\\end{subfigure}",
		"\t\\begin{subfigure}{0.45\\textwidth}",
		"\t\t\\centering",
		"\t\t\\includegraphics[width=\\textwidth]{images/pdf/${4: second figure name}}",
		"\t\t\\caption{${5:sub caption}}",
		"\t\t\\label{subfig_2: ${1:title}}",
		"\t\\end{subfigure}",
		"\t\\caption{Diagram ${6:main caption}.}",
		"\t\\label{fig: ${1:title}}",
		"\\end{figure}"
		],
	},

	"figures_2_column":
	{
		"prefix": "l.figures_two_column",
		"body":
		[
		"\\begin{figure}[H]",
		"\t\\figuretitle{${1:title}}",
